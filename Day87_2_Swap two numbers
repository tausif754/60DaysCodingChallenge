//User function Template for Java

class Solution{
    static List<Integer> get(int a,int b)
    {
        // code here
        ArrayList<Integer> al=new ArrayList<Integer>();
           a= a+b;
           b=a-b;
           a=a-b;
        al.add(a);
        al.add(b);
        return al;
        
    }
}
