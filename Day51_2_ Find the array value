public class FindArrayValue {
    public static void main(String[] args) {

        int[] num = { 11, 28, 34, 40, 45 };
        int toFind = 45;
        boolean found = false;

        for (int n : num) {
            if (n == toFind) {
                found = true;
                break;
            }
        }

        if (found)
            System.out.println(toFind + " is found.");
        else
            System.out.println(toFind + " is not found.");
    }
}
