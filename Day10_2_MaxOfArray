//import java.util.Scanner;

public class MaxOfArray {
    public static void main(String[] args) {

        int[] number = { 100, 20, 50, 220 };
        int result = 0;
        for (int i = 0; i < number.length; i++) {
            if (number[i] > result) {
                result = number[i];
            }
        }
        System.out.println("Largest element present in the given array: " + result);

    }
}
